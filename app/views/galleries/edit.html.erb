<% provide(:title, "Update Gallery") %>

<div class="subtitle"><%=@gallery.title %></div>
<h1>Update your gallery</h1>

<div class="row">
  <div class="span6 offset3">
    <%= form_for(@gallery) do |f| %>
       
        <%=render 'shared/error_messages', thing: @gallery%>
		<%=render 'gallery_form', f:f   , allassets: @allassets , usedsortedassets: @usedsortedassets %>

      <%= f.submit "Save changes", class: "btn btn-large btn-primary" %>
    <% end %>

    
  </div>
</div>